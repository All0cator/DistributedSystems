<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/b_new_purchase_back3"
        android:layout_width="154dp"
        android:layout_height="45dp"
        android:layout_marginTop="456dp"
        android:background="@drawable/enter_button_colour"
        android:text="Back"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.875"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t_main_greetings" />

    <Button
        android:id="@+id/b_new_purchase_back2"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="456dp"
        android:background="@drawable/enter_button_colour"
        android:text="Add Product"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t_main_greetings" />

    <Button
        android:id="@+id/b_new_purchase_back"
        android:layout_width="154dp"
        android:layout_height="45dp"
        android:layout_marginTop="532dp"
        android:background="@drawable/enter_button_colour"
        android:text="Complete"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t_main_greetings" />

    <TextView
        android:id="@+id/t_main_greetings"
        android:layout_width="245dp"
        android:layout_height="68dp"
        android:background="@drawable/login_page_background_colour"
        android:gravity="center"
        android:text="New Purchase"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />

    <ListView
        android:layout_width="382dp"
        android:layout_height="347dp"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="144dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Give Quantity"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="509dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="89dp"
        android:layout_height="34dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/b_new_purchase_back"
        app:layout_constraintTop_toBottomOf="@+id/b_new_purchase_back3"
        app:layout_constraintVertical_bias="0.057" />

</androidx.constraintlayout.widget.ConstraintLayout>